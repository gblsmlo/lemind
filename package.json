{
  "name": "lemind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky",
    "db:drift": "drizzle-kit diff:pg --schema src/infra/db/schema --out src/infra/db/migrations",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:test": "dotenv -e .env.test -- drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:studio:test": "dotenv -e .env.test drizzle-kit studio",
    "db:test:up": "docker-compose up -d db",
    "db:test:down": "docker-compose down",
    "test": "dotenv -e .env.test vitest run",
    "test:watch": "dotenv -e .env.test vitest watch",
    "test:coverage": "dotenv -e .env.test vitest run --coverage",
    "test:integration": "docker-compose up -d db && dotenv -e .env.test vitest run src/**/__integration__/**/*.test.ts && docker-compose down",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:format": "biome format --write .",
    "lint:staged": "biome check . --staged --fix",
    "lint:ci": "biome check --error-on-warnings .",
    "types:gen": "supabase gen types typescript --project-id qkukcnjackyvblabvwwn --schema public,auth > src/shared/types/supabase.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.87.1",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-table": "^8.21.3",
    "@tc96/biome-config": "^2.2.0",
    "@tc96/ui-react": "^0.8.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.555.0",
    "next": "16.0.5",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.1",
    "postgres": "^3.4.7",
    "react": "19.2.0",
    "react-day-picker": "^9.12.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.1",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "uuidv7": "^1.0.2",
    "vaul": "^1.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "drizzle-kit": "^0.31.7",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "biome check --write"
    ],
    "src/**/*.{json}": [
      "biome format --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  }
}
